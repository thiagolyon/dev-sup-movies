package com.devsuperior.lyonmovier.entities;

public class Score {

	private ScorePK id = new ScorePK();
	
	private Double value;	
	
	public Score() {}

	public ScorePK getId() {
		return id;
	}

	public void setId(ScorePK id) {
		this.id = id;
	}

	public Double getValue() {
		return value;
	}

	public void setValue(Double value) {
		this.value = value;
	}
	
}
